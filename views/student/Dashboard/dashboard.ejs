    <!--  CSS for Demo Purpose, don't include it in your project
    <link href="/stylesheets/Dashboard/demo.css" rel="stylesheet" />-->


    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="/stylesheets/Dashboard/pe-icon-7-stroke.css" rel="stylesheet" />

	<link rel='stylesheet prefetch' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css'>

</head>
<body>

<div class="wrapper">
    <div class="sidebar" data-color="purple" data-image="/img/sidebar-5.jpg">

    	<div class="sidebar-wrapper">
            <div class="logo">
                <a class="simple-text">
                    <%= name %>
                </a>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="/dashboard_student">
                        <i class="pe-7s-graph"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <li>
                    <a href="/dashboard_student/user_profile">
                        <i class="pe-7s-user"></i>
                        <p>User Profile</p>
                    </a>
                </li>
                <li>
                    <a href="/dashboard_student/tests_list">
                        <i class="pe-7s-note2"></i>
                        <p>Tests</p>
                    </a>
                </li>
            </ul>
    	</div>
    </div>

    <div class="main-panel">
		<nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">Active Tests</a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="/logout">
                                Log out
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

		<div class = "content">
			<div class = "container-fluid">
				<div class = "row">
					<div class = "col-md-12">

					<a href="#" style = "color:black;">
						<div class="blog-card">
							<div class="photo photo1"></div>
							<ul class="details">
								<li class="author">John Doe</li>
								<li class="date">Aug. 24, 2015</li>
							</ul>
							<div class="description">
								<h1 style="display:block;font-size:2em;font-weight:bold;">
									Learning to Code
								</h1>
								<h2>Opening a door to the future</h2>
								<p class="summary">Lorem ipsum dolor sit amet, consectetur adipisicing elit</p>
							</div>
						</div>
					</a>


					</div>
				</div>
			</div>
		</div>


    </div>
</div>


</body>

    <!--   Core JS Files   -->
    <script src="/js/Dashboard/jquery-1.10.2.js" type="text/javascript"></script>
	<script src="/js/Dashboard/bootstrap.min.js" type="text/javascript"></script>

	<!--  Checkbox, Radio & Switch Plugins -->
	<script src="/js/Dashboard/bootstrap-checkbox-radio-switch.js"></script>

	<!--  Charts Plugin -->
	<script src="/js/Dashboard/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="/js/Dashboard/bootstrap-notify.js"></script>

    <!--  Google Maps Plugin
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"></script> -->

    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
	<script src="/js/Dashboard/light-bootstrap-dashboard.js"></script>

	<!-- Light Bootstrap Table DEMO methods, don't include it in your project!
	<script src="/js/Dashboard/demo.js"></script>-->

